{
    "swagger": "2.0",
    "info": {
        "description": "High performant JKJAV API Server",
        "title": "JKJAV API Server",
        "contact": {},
        "version": "1.0"
    },
    "basePath": "/",
    "paths": {
        "/stations/{name}": {
            "get": {
                "description": "Get station slots by location",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "List Station",
                "parameters": [
                    {
                        "type": "string",
                        "description": "select the location. The only available option is PWTC",
                        "name": "name",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/entities.Station"
                            }
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "entities.Station": {
            "type": "object",
            "properties": {
                "availability": {
                    "type": "integer",
                    "example": 10
                },
                "date": {
                    "type": "string",
                    "example": "20210516"
                },
                "location": {
                    "type": "string",
                    "example": "PWTC"
                }
            }
        }
    }
}